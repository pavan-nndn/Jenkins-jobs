pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                script {
                    echo 'starting the build state'
                    sh 'pwd'
                     //Map servers = readJSON file: serverConfig
                     sshagent(credentials: ["guxgit1"]) {
                     echo 'ssh is working'
                     sh 'ssh -i'
                     }
                     //echo servers
                     echo 'ending the build state'
                     
                }
            }
        }
    }
}
