pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                script {
                    
                    echo 'starting the build state'
                    configFileProvider(
      [configFile(fileId: 'ServerConfig', variable: 'serverConfig')]) {
                    def servers = readJSON file: serverConfig
                    def author = servers['prod']['author']
                    echo author
                    }
                }
            }
        }
    }
}
