pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                script {
                    echo 'starting the build state'
                    sh 'pwd'
                     configFileProvider(
                            [configFile(fileId: 'serverConfig', variable: 'serverConfig')]) {
                        Map servers = readJSON file: serverConfig
                        
                        echo servers
                     echo 'ending the build state'
                     }
                }
            }
        }
    }
}
